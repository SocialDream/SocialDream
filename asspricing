                                <div class="switchWrapper center js-switch">
                                    <div style="pointer-events: all;cursor: pointer;" onclick="javascript:if(document.getElementById('planToggleInput').checked)document.getElementById('planToggleInput').click();" class="text colorOne left active">Monthly</div>
                                    <label class="switchPricing switch js-pricingToggle">
                                        <input id="planToggleInput" type="checkbox" class="switchInput"><span class="slider round colorOne"></span></label>
                                    <div style="pointer-events: all;cursor: pointer;" onclick="javascript:if(!document.getElementById('planToggleInput').checked)document.getElementById('planToggleInput').click();" class="middle colorTwo right"><span class="text">Annual</span><span class="additional">(Save 35%)</span></div>
                                </div>
                                <script>
                                    var a = document.querySelectorAll(".js-switch");
                                    for (const t of a) {
                                        var e = t.querySelector("input"),
                                            n = t.querySelector(".left"),
                                            i = t.querySelector(".right"),
                                            s = document.querySelectorAll(".slider")[0];
                                        if (e.checked) {
                                            i.classList.add("active");
                                            s.classList.add("active");
                                        } else
                                            n.classList.add("active");
                                        t.addEventListener("click", function() {
                                            var t = e.checked;
                                            n.classList.toggle("active", !t);
                                            i.classList.toggle("active", t);
                                            s.classList.toggle("active", t);
                                            document.querySelectorAll(".annualPricingWrapper")[0].style.display = t ? "flex" : 'none';
                                            document.querySelectorAll(".monthlyPricingWrapper")[0].style.display = t ? "none" : 'flex';
                                        })
                                    }
                                </script>
                            </div>
